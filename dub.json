{
	"name": "dub-registry",
	"description": "Online registry for dub packages",
	"homepage": "http://code.dlang.org/",
	"license": "GPL-3.0",
	"authors": [
		"Sönke Ludwig",
		"Matthias Dondorff"
	],
	"dependencies": {
		"userman": "~>0.3.0",
		"vibe-d": "~>0.7.26-alpha.1",
		"dub": "~>0.9.22"
	},
	"subConfigurations": {
		"dub": "library-nonet"
	},
	"versions": ["VibeJsonFieldNames"],
	"configurations": [
		{
			"name": "application",
			"targetType": "executable",
			"mainSourceFile": "source/app.d",
			"versions": ["VibeDefaultMain"]
		},
		{
			"name": "library",
			"targetType": "library",
			"excludedSourceFiles": ["source/app.d"]
		}
	]
}
